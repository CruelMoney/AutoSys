<Page
    x:Class="StudyConfigurationUI.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:pickers="using:Windows.Storage.Pickers"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource AppBarBackgroundThemeBrush}">
        <StackPanel x:Name="contentPanel" Margin="8,32,0,0">
            <Button Click="OnOpenStudy">Open Existing Study</Button>
            <Button Click="OnOpenStudyWithStudy">LavetStudy</Button>
            <Button Click="PostStudy">Post A study</Button>
            <Button Click="callAPI">StartSTUDYFROM AN AP AI</Button>
            
        </StackPanel>
    </Grid>
</Page>